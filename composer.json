{
    "name": "crell/transformer-bundle",
    "description": "Symfony Bridge for the Crell Transformer library.",
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Larry Garfield",
            "email": "larry@garfieldtech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Crell\\TransformerBundle\\": "src/",
            "Crell\\TransformerBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/dependency-injection": ">=2.3",
        "crell/transformer": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Crell/Transformer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Crell/TransformerBundle"
        }
    ],
    "minimum-stability": "dev"

}
